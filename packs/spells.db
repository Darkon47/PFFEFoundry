":true},"concentration":{"value":null,"_deprecated":true},"ritual":{"value":null,"_deprecated":true},"prepared":{"value":null,"_deprecated":true},"attack":{"parts":[]},"weaponData":{"critRange":20,"critMult":2},"learnedAt":{"class":[["antipaladin",2],["bard",2],["cleric/oracle",2],["inquisitor",2],["mesmerist",2],["psychic",2]]}},"sort":700001,"flags":{},"img":"systems/pf1/icons/skills/violet_20.jpg"}
